{
  "name": "@victortolbert/starter-kit",
  "version": "0.1.0",
  "packageManager": "pnpm@8.11.0",
  "description": "Rapidly build prototypes using Gulp and Nunjucks",
  "author": "Victor Tolbert <victor.tolbert@gmail.com> (http://victortolbert.com/)",
  "license": "MIT",
  "homepage": "https://starter-kit.victortolbert.com",
  "repository": "https://github.com/victortolbert/starter-kit",
  "bugs": {
    "url": "https://github.com/victortolbert/starter-kit/issues"
  },
  "keywords": [
    "design-system",
    "starter",
    "gulp",
    "nunjucks"
  ],
  "main": "",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "test": "gulp",
    "dev": "gulp",
    "build": "gulp build",
    "clean": "gulp clean",
    "prestart": "npm run build && node server",
    "lint:styles": "stylelint src/styles/**/*.{css,scss} --fix --ignore-path .gitignore",
    "lint:js": "eslint .",
    "format": "eslint . --fix",
    "lint:debug": "eslint --debug .",
    "check:types": "vue-tsc --noEmit",
    "check:bundle": "npx vite-bundle-visualizer",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "release": "release-it"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "2.23.0-fix",
    "gulp-cli": "^2.3.0",
    "http-auth": "^4.2.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@antfu/eslint-plugin-prettier": "5.0.1-1",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^20.11.5",
    "@unocss/eslint-config": "^0.58.3",
    "browser-sync": "^3.0.2",
    "changelogen": "^0.5.5",
    "changelogithub": "^0.13.3",
    "child_process": "^1.0.2",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.4",
    "commitlint-config-cz": "0.13.3",
    "commitlint-config-unjs": "^1.0.1",
    "concurrently": "8.2.2",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-github-releaser": "^3.1.5",
    "conventional-recommended-bump": "^9.0.0",
    "del": "^6.1.1",
    "delete": "^1.1.0",
    "dotenv": "^16.3.2",
    "downloadjs": "^1.4.7",
    "dprint": "^0.45.0",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "events": "^3.3.0",
    "execa": "^8.0.1",
    "express": "^4.18.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-data": "^1.3.1",
    "gulp-if": "^3.0.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-nunjucks": "5.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.0",
    "pnpm": "8.14.1",
    "postcss-lit": "^1.1.1",
    "prettier": "^3.2.4",
    "release-it": "^17.0.1",
    "rollup": "^4.9.6",
    "rxjs": "^7.8.1",
    "sass": "^1.70.0",
    "simple-git": "^3.22.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "stylelint-8-point-grid": "^2.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-slds": "^1.0.8",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.3.1",
    "taze": "^0.13.0",
    "through2": "^4.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "unocss": "^0.58.3",
    "util": "^0.12.5"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commitlint --edit $1",
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint . --fix",
    "*.{css,vue}": "stylelint"
  },
  "browserslist": [
    "Edge 16"
  ]
}
